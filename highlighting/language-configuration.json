{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}",
            "notIn": ["string", "comment"]
        },
        {
            "open": "[",
            "close": "]",
            "notIn": ["string", "comment"]
        },
        {
            "open": "(",
            "close": ")",
            "notIn": ["string", "comment"]
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": ["string", "comment"]
        },
        {
            "open": "'",
            "close": "'",
            "notIn": ["string", "comment"]
        }
    ],
    "folding": {
        "markers": {
            "start": "^\\s*\\{\\s*$",
            "end": "^\\s*\\}\\s*$"
        }
    }
}